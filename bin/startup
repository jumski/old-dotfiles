#!/bin/bash

$HOME/dotfiles/bin/setup_keyboard

# prevent mouse clicks
# syndaemon -i 1.5 -t &

mocp --server || rm ~/.moc/pid && mocp --server

PULSE_LATENCY_MSEC=30 skype &

pidgin &

primus-chrome &

gnome-terminal &

supervisord -c $HOME/dotfiles/conf/supervisord.conf &

echo "`date` DONE!" >> ~/.startup.log
