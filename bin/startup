#!/bin/bash

# set keyboard repeat
xset r rate 250 100

# remap caps and right win key
setxkbmap -option ctrl:nocaps
setxkbmap -option altwin:ctrl_win

# prevent mouse clicks
syndaemon -i 1.5 -t &

# mocp
mocp --server

# skype
skype &

# pidgin
pidgin &

# chrome
google-chrome &

# urxvt
urxvt &

# supervisor
supervisord -c $HOME/dotfiles/conf/supervisord.conf
